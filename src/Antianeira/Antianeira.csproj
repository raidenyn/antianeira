<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup>
    <RuntimeIdentifiers>win8-x64</RuntimeIdentifiers>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netcoreapp2.0\Antianeira.xml</DocumentationFile>
    <NoWarn>1701;1702;1705;1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Templates\class.liquid" />
    <None Remove="Templates\client_method.liquid" />
    <None Remove="Templates\interface_method_parameter.liquid" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Templates\client.liquid" />
    <EmbeddedResource Include="Templates\client_method.liquid" />
    <EmbeddedResource Include="Templates\definitions.liquid" />
    <EmbeddedResource Include="Templates\enum_field.liquid" />
    <EmbeddedResource Include="Templates\interface_method.liquid" />
    <EmbeddedResource Include="Templates\interface_method_parameter.liquid" />
    <EmbeddedResource Include="Templates\property.liquid" />
    <EmbeddedResource Include="Templates\type.liquid" />
    <EmbeddedResource Include="Templates\enum.liquid" />
    <EmbeddedResource Include="Templates\comment.liquid" />
    <EmbeddedResource Include="Templates\interface.liquid" />
    <EmbeddedResource Include="Templates\class.liquid" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DotLiquid" Version="2.0.180" />
    <PackageReference Include="JetBrains.Annotations" Version="11.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.0.0" />
    <PackageReference Include="Microsoft.Extensions.CommandLineUtils" Version="1.1.1" />
  </ItemGroup>

</Project>